<?xml version='1.0' encoding='UTF-8'?>
<types:mule xmlns:types="http://www.mulesoft.org/schema/mule/types">
  <types:catalog>
    <types:type name="input" format="csv">
      <types:shape format="weave" example="examples/fullname-3.csv"><![CDATA[%dw 2.0
type input = Array<{|
  id: String, 
  gender: String, 
  name: String, 
  age: String
|}> {"MetadataFormat": "csv"}

]]></types:shape>
      <types:properties>
        <types:property name="headerLineNumber" value="0"/>
        <types:property name="header" value="true"/>
        <types:property name="isQuotedString" value="false"/>
      </types:properties>
    </types:type>
  </types:catalog>
  <types:enrichment select="#8ac4f8d3-ffcd-4391-a08f-649c0e15a2e9">
    <types:processor-declaration>
      <types:input-event>
        <types:message>
          <types:payload type="input"/>
        </types:message>
      </types:input-event>
    </types:processor-declaration>
  </types:enrichment>
</types:mule>